<nav aria-label="Page navigation example">
  <ul class="pagination">
  	<% if page > first_page %>
	  	<li class="page-link">
		      <%= link_to send(url_custom_helper_name, {page: page - 1}) do %> 
			        <span aria-hidden="true">&laquo;</span>
			        <span class="sr-only">Previous</span>
		      <% end %>
		</li>
	<% end %>
	
    <% (first_page..last_page).each do |page_number| %>
    	<li class="page-link">
    		<%= link_to  page_number, send(url_custom_helper_name, {page: page_number})%> 
    	</li>
	<% end %>
  
	<% if page < last_page %>
	    <li class="page-link">
	        <%= link_to send(url_custom_helper_name, {page: page + 1}) do %>
	        	<span aria-hidden="true">&raquo;</span>
	        	<span class="sr-only">Next</span>
	        <% end %>
	    </li>
	<% end %>
  </ul>
</nav>